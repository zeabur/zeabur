{
    "$schema": "./schema.json",
    "code": "maloja",
    "name": "Maloja",
    "description": "Maloja is a Self-hosted music scrobble database to create personal listening statistics and charts.",
    "icon": "https://maloja.krateng.ch/favicon.ico",
    "image": "krateng/maloja:latest",
    "command": null,
    "args": null,
    "port": 42010,
    "dataPath": "/var/lib/maloja",
    "network": "HTTP",
    "variables": [
      {
        "name": "MALOJA_DATA_DIRECTORY",
        "value": "/var/lib/maloja"
      },
      {
        "name": "MALOJA_FORCE_PASSWORD",
        "value": ""
      }
    ]
  }