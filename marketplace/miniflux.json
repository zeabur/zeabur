{
    "$schema": "./schema.json",
    "code": "miniflux",
    "name": "Miniflux",
    "description": "Miniflux is a minimalist and opinionated feed reader.",
    "icon": "https://miniflux.app/favicon.ico",
    "image": "miniflux/miniflux:latest",
    "command": null,
    "args": null,
    "port": 80,
    "network": "HTTP",
    "variables": [
      {
        "name": "DATABASE_URL",
        "value": "${POSTGRES_HOST}"
      },
      {
        "name": "PROXY_IMAGES",
        "value": "all"
      },
      {
        "name": "RUN_MIGRATIONS",
        "value": "1"
      },
      {
        "name": "BASE_URL",
        "value": "https://your-domain.tld"
      },
      {
        "name": "ADMIN_PASSWORD",
        "value": ""
      },
      {
        "name": "ADMIN_USERNAME",
        "value": ""
      },
      {
        "name": "CREATE_ADMIN",
        "value": "1"
      }
    ]
  }